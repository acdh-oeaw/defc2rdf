<rdf:Description rdf:about="http://vocab.acdh/defc2cidoc#area/area/82">
	<rdf:type rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E27_Site"/>
	<P90_has_value xmlns="http://www.cidoc-crm.org/cidoc-crm/" xml:lang="en">Around the fire places pottery, tools, shells and animal bones were found.</P90_has_value>
	<P89_falls_within xmlns="http://www.cidoc-crm.org/cidoc-crm/" rdf:resource="http://vocab.acdh/defc2cidoc#site/site/43"/>
